curl -X POST http://localhost:5002/encode_board -H "Content-Type: application/json" -d '{"fen": "8/pppppppp/8/8/8/8/PPPPPPPP/8 w KQkq - 0 1"}'
curl -X POST http://localhost:5002/encode_board -H "Content-Type: application/json" -d '{"fen": "rnbqkbnr/pppppppp/8/8/8/8/PPPPPPPP/RNBQKBNR w KQkq - 0 1"}'
